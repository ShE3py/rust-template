{% if workspace %}[workspace]
members = [
    "{{ project-name }}",
]
resolver = "2"

[workspace.package]
{% else %}[package]
name = "{{ project-name }}"
version = "0.0.0"
authors = ["{{ username }}"]
{% endif %}repository = "https://github.com/{{ github }}/{{ project-name }}/"
license = "MIT OR Apache-2.0"
{% unless workspace %}categories = []
keywords = []
{% endunless %}edition = "2021"{% unless workspace %}
rust-version = "1.74"
publish = false

[package.metadata.docs.rs]
all-features = true
targets = []
rustdoc-args = ["--cfg", "nightlydoc"]{% endunless %}
